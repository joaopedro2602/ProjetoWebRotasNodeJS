 <div class="row">
        
        <div class="col">
            {{#each post}}
            {{#dataValues}}
            <form method="post" action="/atualizar" id="form" name="form">
                <div class="row g-3">

                    <div style="text-align: left;" class="col-md-12">
                        <input type="hidden" name="id" class="form-control" id="nome" value="{{id}}">
                        <label for="inputEmail4" class="form-label"> Nome: </label>
                        <input autofocus type="text" placeholder="ex: Alex" class="form-control" name="txtNome" required
                           value="{{nome}}" id="txtNome">
                    </div>

                    <div style="text-align: left;" class="col-md-612">
                        <label for="inputTelefone4" class="form-label">Telefone:</label>
                        <input type="text" placeholder="example@gmail.com" class="form-control" name="txtTelefone"
                           value="{{telefone}}" required id="txtTelefone">
                    </div>

                    <div style="text-align: left;" class="col-md-12">
                        <label for="inputCep4" class="form-label">Origem:</label>
                        <select class="form-select" id="inputGroupSelect01">
                            <option selected>Escolha um método de
                                contato...</option>
                            <option value="1">Celular</option>
                            <option value="2">Telefone</option>
                        </select>
                    </div>

                    <div style="text-align: left;" class="col-md-12">
                        <label for="inputBairro4" class="form-label">Data de Contato:</label>
                        <input type="date" class="form-control" required value="{{data_contato}}" name="txtDataContato" id="txtDataContato">
                    </div>

                    <div style="text-align: left;" class="col-md-12">
                        <label for="inputEndereco4" class="form-label">Obersevação:</label>
                        <textarea class="form-control" name="txtObservacao" id="txtObservacao" rows="6">{{observacao}}</textarea>
                    </div>


                    <div style="text-align: left;" class="col-12">
                        <button type="submit" id="btnCadastrar" class="btn btn-dark">Cadastrar</button>
                    </div>
            </form>
            {{/dataValues}}
            {{/each}}
        </div>
    </div>
</div>

</div>